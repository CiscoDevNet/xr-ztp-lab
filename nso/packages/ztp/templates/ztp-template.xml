<config-template xmlns="http://tail-f.com/ns/config/1.0">
<devices xmlns="http://tail-f.com/ns/ncs">
  <device tags="merge">
    <name>{$DEVICE_NAME}</name>
    <address>{$ADDRESS}</address>
    <authgroup>default</authgroup>
    <device-type>
      <cli>
        <ned-id xmlns:cisco-iosxr-cli-7.15="http://tail-f.com/ns/ned-id/cisco-iosxr-cli-7.15">cisco-iosxr-cli-7.15:cisco-iosxr-cli-7.15</ned-id>
      </cli>
    </device-type>
    <state>
      <admin-state>unlocked</admin-state>
    </state>
  </device>
</devices>
</config-template>
